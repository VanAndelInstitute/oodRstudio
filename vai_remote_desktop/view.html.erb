<form action="/pun/sys/dashboard/noVNC-1.3.0/vnc.html" method="get" target="_blank">
  <input type="hidden" name="path" value="rnode/<%= host %>/<%= port %>/websockify"/>
  <input type="hidden" name="username" value="<%= ENV["USER"] %>">
  <input type="hidden" name="password" value="<%= password %>">
  <input type="hidden" name="autoconnect" value="true">
  <input type="hidden" name="appUri" value="">
  <button class="btn btn-primary" type="submit">
    <i class="fa fa-registered"></i> Connect to instrument 
  </button>
</form>
